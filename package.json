{
  "name": "elite-journal",
  "productName": "Elite Journal",
  "version": "1.6.22",
  "description": "Parsing the elite dangerous journal and putting it in a cool format!",
  "license": "MIT",
  "repository": "willyb321/elite-journal",
  "author": {
    "name": "willyb321",
    "email": "wbwilliam7@gmail.com",
    "url": "tehsuperwilly.tech"
  },
  "scripts": {
    "test": "jake test",
    "start": "electron .",
    "build": "electron-zip-packager . --out=dist --overwrite --platform=win32 --arch=x64 --prune --asar",
    "pack": "jake pack",
    "dist": "jake build",
    "release": "jake release",
    "tag": "release-it"
  },
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "bugsnag": "^1.8.0",
    "debug": "^2.2.0",
    "electron-is-dev": "^0.1.2",
    "fs.extra": "^1.3.2",
    "jsonfile": "^2.4.0",
    "line-by-line": "^0.1.5",
    "node-watch": "^0.4.1",
    "source-map-support": "^0.4.6",
    "tableify": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.4.5",
    "electron-builder": "^7.23.1",
    "electron-zip-packager": "^4.0.0",
    "mocha": "^3.1.2",
    "mocha-circleci-reporter": "^0.0.2",
    "release-it": "^2.4.3",
    "spectron": "^3.4.0",
    "xo": "^0.17.0",
    "jake": "latest"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ],
    "ignores": [
      "tests/*",
      "test.js"
    ]
  },
  "build": {
    "appId": "com.willyb321.elite-journal",
    "nsis": {
      "oneClick": true
    },
    "win": {
      "target": [
        "nsis"
      ],
      "publish": [
        "bintray",
        "github"
      ],
      "iconUrl": "https://github.com/willyb321/elite-journal/blob/master/build/icon.ico?raw=true",
      "remoteReleases": true
    }
  }
}
